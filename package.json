{
  "name": "@bobaboard/ao3.js",
  "version": "0.6.7",
  "description": "A NodeJS API for scraping AO3 data",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist/ && tsup src/index.ts --format esm,cjs --dts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --no-cache .test.ts$ --verbose=true",
    "encode-mock-files": "ts-node-esm scripts/encode-mock-files.mts"
  },
  "repository": "https://github.com/essential-randomness/AO3.js.git",
  "author": "Essential Randomness <essential.randomn3ss@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.4",
    "directory-tree": "^3.3.0",
    "filenamify": "^5.1.1",
    "jest": "^29.6.0",
    "jest-esm-transformer": "^1.0.0",
    "msw": "^0.39.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "cheerio": "^1.0.0-rc.10"
  }
}
